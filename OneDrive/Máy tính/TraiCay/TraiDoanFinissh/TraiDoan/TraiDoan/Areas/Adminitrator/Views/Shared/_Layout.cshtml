<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" href="images/favicon.ico" type="image/ico" />
    <title>Shop Food Admin | </title>
    <!-- Bootstrap -->
    <link href="~/Areas/Adminitrator/Content/vendors/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <!-- Font Awesome -->
    <link href="~/Areas/Adminitrator/Content/vendors/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <!-- NProgress -->
    <link href="~/Areas/Adminitrator/Content/vendors/nprogress/nprogress.css" rel="stylesheet">
    <!-- iCheck -->
    <link href="~/Areas/Adminitrator/Content/vendors/iCheck/skins/flat/green.css" rel="stylesheet">

    <!-- bootstrap-progressbar -->
    <link href="~/Areas/Adminitrator/Content/vendors/bootstrap-progressbar/css/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet">
    <!-- JQVMap -->
    <link href="~/Areas/Adminitrator/Content/vendors/jqvmap/dist/jqvmap.min.css" rel="stylesheet" />
    <!-- bootstrap-daterangepicker -->
    <link href="~/Areas/Adminitrator/Content/vendors/bootstrap-daterangepicker/daterangepicker.css" rel="stylesheet">
    <!-- Custom Theme Style -->
    <link href="~/Areas/Adminitrator/Content/build/css/custom.min.css" rel="stylesheet">
</head>
<body class="nav-md">
    <div class="container body">
        <div class="main_container">
            <div class="col-md-3 left_col">
                <div class="left_col scroll-view">
                    <div class="navbar nav_title" style="border: 0;">
                        <a href="/Adminitrator/Home/Index.cshtml" class="site_title"><i class="fa fa-paw"></i> <span>Admin</span></a>
                    </div>
                    <div class="clearfix"></div>
                    <!-- menu profile quick info -->
                    <div class="profile clearfix">
                        <div class="profile_pic">
                            <img src="~/Areas/Adminitrator/Content/images/user.png" alt="..." class="img-circle profile_img">
                        </div>
                        <div class="profile_info">
                            <span>Welcome</span>
                            <h2>Admin</h2>
                        </div>
                    </div>
                    <!-- /menu profile quick info -->
                    <br />
                    <!-- sidebar menu -->
                    <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
                        <div class="menu_section">
                            <h3>Danh mục quản lý</h3>
                            <ul class="nav side-menu">
                                <li style="background-color:aqua;"><a style="color:black" href="/Home/Index"><i class="fa fa-home"></i>HomePage</a></li>
                                <li><a href="/Categories/Index"><i class="fa fa-book"></i>Caterogy</a></li>
                                <li><a href="/Products/Index"><i class="fa fa-product-hunt"></i>Product</a></li>
                                <li><a href="/Orders/Index"><i class="fa fa-pencil"></i>Oder</a></li>
                                <li><a href="/OrderDetails/Index"><i class="fa fa-pencil"></i>OrderDetail</a></li>
                                <li><a href="/Users/Index"><i class="fa fa-users"></i>User</a></li>
                                <li><a href="/blog_category/Index"><i class="fa fa-users"></i>CateBlog</a></li>
                                <li><a href="/blogs/Index"><i class="fa fa-users"></i>blogs</a></li>
                                <li><a href="@Url.Action("LogOut","Home")"><span class="glyphicon glyphicon-off" style="color:red;"></span>LogOut</a></li>
                            </ul>
                        </div>
                    </div>
                    <!-- /sidebar menu -->
                    <!-- /menu footer buttons -->
                    <div class="sidebar-footer hidden-small">
                        <a data-toggle="tooltip" data-placement="top" title="Settings">
                            <span class="glyphicon glyphicon-cog" aria-hidden="true"></span>
                        </a>
                        <a data-toggle="tooltip" data-placement="top" title="FullScreen">
                            <span class="glyphicon glyphicon-fullscreen" aria-hidden="true"></span>
                        </a>
                        <a data-toggle="tooltip" data-placement="top" title="Lock">
                            <span class="glyphicon glyphicon-eye-close" aria-hidden="true"></span>
                        </a>
                        <a style="color:black; background-color:white" data-toggle="tooltip" data-placement="top" title="Logout" href="/Home/Index">
                            <span class="glyphicon glyphicon-off" aria-hidden="true"></span>
                        </a>
                    </div>
                    <!-- /menu footer buttons -->
                </div>
            </div>
            <!-- top navigation -->
            <div class="top_nav">
                <div class="nav_menu">
                    <nav>
                        <div class="nav toggle">
                            <a id="menu_toggle"><i class="fa fa-bars"></i></a>
                        </div>
                        <ul class="nav navbar-nav navbar-right">
                            <li class="">
                                <a href="javascript:;" class="user-profile dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                    <img src="~/Areas/Adminitrator/Content/images/user.png" alt="">Admin @Session["Admin"]
                                </a>
                            </li>
                        </ul>
                    </nav>
                </div>
            </div>
            <!-- /top navigation -->
            <!-- page content -->
            
            <div class="right_col" role="main">
                @RenderBody();
            </div>
            <!-- /page content -->
            <!-- footer content -->
            <footer>
                <div class="pull-right">
                    Gentelella - Bootstrap Admin Template by <a href="https://colorlib.com">Colorlib</a>
                </div>
                <div class="clearfix"></div>
            </footer>
            <!-- /footer content -->
        </div>
    </div>
    <!-- jQuery -->
    <script src="~/Areas/Adminitrator/Content//vendors/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap -->
    <script src="~/Areas/Adminitrator/Content/vendors/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- FastClick -->
    <script src="~/Areas/Adminitrator/Content/vendors/fastclick/lib/fastclick.js"></script>
    <!-- NProgress -->
    <script src="~/Areas/Adminitrator/Content/vendors/nprogress/nprogress.js"></script>
    <!-- Chart.js -->
    <script src="~/Areas/Adminitrator/Content/vendors/Chart.js/dist/Chart.min.js"></script>
    <!-- gauge.js -->
    <script src="~/Areas/Adminitrator/Content/vendors/gauge.js/dist/gauge.min.js"></script>
    <!-- bootstrap-progressbar -->
    <script src="~/Areas/Adminitrator/Content/vendors/bootstrap-progressbar/bootstrap-progressbar.min.js"></script>
    <!-- iCheck -->
    <script src="~/Areas/Adminitrator/Content/vendors/iCheck/icheck.min.js"></script>
    <!-- Skycons -->
    <script src="~/Areas/Adminitrator/Content/vendors/skycons/skycons.js"></script>
    <!-- Flot -->
    <script src="~/Areas/Adminitrator/Content/vendors/Flot/jquery.flot.js"></script>
    <script src="~/Areas/Adminitrator/Content/vendors/Flot/jquery.flot.pie.js"></script>
    <script src="~/Areas/Adminitrator/Content/vendors/Flot/jquery.flot.time.js"></script>
    <script src="~/Areas/Adminitrator/Content/vendors/Flot/jquery.flot.stack.js"></script>
    <script src="~/Areas/Adminitrator/Content/vendors/Flot/jquery.flot.resize.js"></script>
    <!-- Flot plugins -->
    <script src="~/Areas/Adminitrator/Content/vendors/flot.orderbars/js/jquery.flot.orderBars.js"></script>
    <script src="~/Areas/Adminitrator/Content/vendors/flot-spline/js/jquery.flot.spline.min.js"></script>
    <script src="~/Areas/Adminitrator/Content/vendors/flot.curvedlines/curvedLines.js"></script>
    <!-- DateJS -->
    <script src="~/Areas/Adminitrator/Content/vendors/DateJS/build/date.js"></script>
    <!-- JQVMap -->
    <script src="~/Areas/Adminitrator/Content/vendors/jqvmap/dist/jquery.vmap.js"></script>
    <script src="~/Areas/Adminitrator/Content/vendors/jqvmap/dist/maps/jquery.vmap.world.js"></script>
    <script src="~/Areas/Adminitrator/Content/vendors/jqvmap/examples/js/jquery.vmap.sampledata.js"></script>
    <!-- bootstrap-daterangepicker -->
    <script src="~/Areas/Adminitrator/Content/vendors/moment/min/moment.min.js"></script>
    <script src="~/Areas/Adminitrator/Content/vendors/bootstrap-daterangepicker/daterangepicker.js"></script>
    <!-- Custom Theme Scripts -->
    <script src="~/Areas/Adminitrator/Content/build/js/custom.min.js"></script>
    <script src="~/Areas/Adminitrator/Content/build/js/Script.js"></script>
</body>
